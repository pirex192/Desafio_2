    "frm.grid()\n",
    "ttk.Label(frm, text=\"Hello World!\").grid(column=10, row=10)#\n",
    "ttk.Button(frm, text=\"Quit\", command=root.destroy).grid(column=50, row=50)\n",
    "root.mainloop()\n"
